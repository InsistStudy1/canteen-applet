Page {
  background-color: #f9f9f9;
}
.select-order-info-container {
  margin: 14rpx;
}
.panel {
  padding: 0 25rpx;
  background-color: #fff;
  border-radius: 5rpx;
}
.price {
   padding-top: 10rpx;
   font-size: 34rpx;
   color: #000;
 }
.price .mark {
  font-size: 24rpx;
}
.big-title {
  font-size: 50rpx;
  line-height: 60rpx;
  padding: 20rpx;
}
/* ----------------------------------- 订单列表 Start ----------------------------------- */
.panel-container {
  margin-top: 14rpx;
}
/*头部*/
.panel-container .header {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #000;
  border-bottom: 1px solid #eee;
  font-weight: 600;
}
/*列表内容*/
.panel-container .list {
  padding: 15rpx 0;
}
.panel-container .list .food-item {
  padding: 15rpx 0;
  display: flex;
}
.panel-container .list .food-item image{
  width: 125rpx;
  height: 125rpx;
}
.panel-container .list .food-item .content {
  flex: 1;
  padding: 5rpx 0 5rpx 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.panel-container .list .food-item .content .title {
  font-size: 28rpx;
  font-weight: 600;
  color: #333;
}
.panel-container .list .food-item .content .num {
  font-size: 24rpx;
  color: #666;
}
.panel-container .list .food-item .price {
  padding-top: 10rpx;
}
/*额外信息*/
.panel-container .detail .detil-item {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #333;
  border-top: 1px solid #eee;
  display: flex;
  justify-content: space-between;
}
/* ----------------------------------- 订单列表 Start ----------------------------------- */
.panel-container .content .item {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
}
.panel-container .content .item .left {
  font-size: 24rpx;
  color: #666;
}
.panel-container .content .item .right {
  font-size: 26rpx;
  color: #000;
}
